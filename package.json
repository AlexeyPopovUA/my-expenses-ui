    "karma": "6.3.16",
    "karma-chrome-launcher": "3.0.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.6",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "standard-changelog": "2.0.11",
    "stylelint": "10.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-language-service": "0.9.9",
    "typescript": "3.5.3",
    "typogr": "0.6.8",
