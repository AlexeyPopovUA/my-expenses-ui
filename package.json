    "lodash": "4.17.15",
    "ng2-ckeditor": "1.2.3",
    "ng2-completer": "3.0.2",
    "ng2-handsontable": "2.0.0",
    "ng2-slim-loading-bar": "4.0.0",
    "ng2-smart-table": "^1.5.0",
    "ng2-tree": "2.0.0-alpha.5",
    "ngx-uploader": "7.1.0",
    "normalize.css": "8.0.1",
    "roboto-fontface": "0.10.0",
